{
	"name": "dasted",
	"description": "A minimal D application.",
	"copyright": "Copyright Â© 2015, cleem",
	"authors": ["cleem"],
	"targetPath": "./bin",
	"sourcePaths": ["src"],
	"debugVersions": [],
	"importPaths": [
		"./src/"
	],
	"dependencies": {
		"libdparse": "~master",
		"msgpack-d": "~master"
	},
	"buildRequirements": [
		"silenceWarnings"
	],
	"configurations": [
		{
			"name": "server",
			"targetType": "executable"
		},
		{
			"name": "module-ast",
			"targetType": "executable",
			"targetName": "module-ast",
			"sourceFiles": ["./sample/module_ast.d"],
			"excludedSourceFiles": ["src/app.d"]
		},
		{
			"name": "run-tests",
			"targetType": "executable",
			"targetName": "run-tests",
			"sourceFiles": ["./test/run_tests.d",
					"./test/test_common.d",
					"./test/complete_simple.d",
					"./test/find_declaration_simple.d"],
			"excludedSourceFiles": ["src/app.d"]
		}
	]
}
